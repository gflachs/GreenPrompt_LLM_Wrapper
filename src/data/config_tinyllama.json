{
    "modeltyp": "text-generation",
    "model": "TinyLlama/TinyLlama-1.1B-Chat-v1.0",
    "uses_chat_template": true,
    "args": {
        "prompting": {
            "max_new_tokens": 256, 
            "do_sample": true,
            "temperature": 0.7,
            "top_k": 50, 
            "top_p": 0.95},
        "deployment": {
            "torch_dtype": "torch.bfloat16",
            "device_map": "auto"}
    }
  }